<suite name="Gigsberg" parallel="true" thread-count="5">
	<listeners>
		<listener class-name="reportConfig.ExtentTestListener" />
	</listeners>
	<parameter name="url" value="https://stage.gigsberg.com/" />
	<test name="Run on CHROME">
		<parameter name="browser" value="chrome" />
		<packages>
			<package name="com.gigsberg.regression" />
		</packages>
	</test>
</suite> <!-- Suite -->