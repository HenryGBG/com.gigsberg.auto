<suite name="Gigsberg" parallel="false" thread-count="5">
	<listeners>
		<listener class-name="reportConfig.ExtentTestListener" />
	</listeners>
	<parameter name="url" value="https://gigsberg.com/" />

	<test name="Run on CHROME">
		<parameter name="browser" value="h_chrome" />
		<classes>
			<class name="com.gigsberg.regression.Testcase_Theatre_Regression" />
			<class name="com.gigsberg.regression.Testcase_Concerts_Regression" />
		</classes>
	</test>

</suite> <!-- Suite -->